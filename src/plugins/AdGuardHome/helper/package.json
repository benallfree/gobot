{
  "name": "gobot-AdGuardHome",
  "version": "0.108.0-b.53",
  "description": "AdGuardHome helper for Gobot",
  "type": "module",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "prepack": "rimraf -v --glob \"gobot-*\" pb_data",
    "dev": "node ./src/cli.js",
    "test": "npm pack && npm rm -g gobot-AdGuardHome && npm i -g gobot-AdGuardHome-`cat package.json | fx '.version'`.tgz && AdGuardHome --g-refresh --g-vvv --help",
    "build": "npm run clean && tsup-node src/cli.ts src/api.ts --dts src/api.ts --format esm"
  },
  "bin": {
    "AdGuardHome": "./dist/cli.js"
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/benallfree/gobot/tree/main/src/plugins/AdGuardHome/helper"
  },
  "keywords": [
    "gobot",
    "AdGuardHome"
  ],
  "author": "Ben Allfree",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benallfree/gobot/issues"
  },
  "homepage": "https://github.com/benallfree/gobot/tree/main/src/plugins/AdGuardHome/helper/readme.md",
  "dependencies": {
    "commander": "^12.0"
  },
  "peerDependencies": {
    "gobot": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  }
}
