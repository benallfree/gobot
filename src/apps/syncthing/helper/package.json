{
  "name": "gobot-syncthing",
  "version": "1.27.6-rc.1-alpha.21",
  "description": "syncthing helper for Gobot",
  "type": "module",
  "main": "dist/api.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsx ./src/cli.ts",
    "build": "npm run clean && tsup-node src/cli.ts src/api.ts --dts src/api.ts --format esm"
  },
  "bin": {
