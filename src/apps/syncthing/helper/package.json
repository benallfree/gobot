  },
  "keywords": [
    "gobot",
    "syncthing",
    "syncthing"
  ],
  "author": "Ben Allfree",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benallfree/gobot/issues"
  },
  "homepage": "https://www.npmjs.com/package/gobot-syncthing",
  "dependencies": {
    "gobot": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  }
}
