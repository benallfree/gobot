{
  "name": "gobot-age",
  "version": "1.1.1-alpha.16",
  "description": "Age helper for Gobot",
  "type": "module",
  "main": "dist/api.js",
  "scripts": {
    "clean": "rimraf dist",
    "prepack": "rimraf -v --glob \"gobot-*\" pb_data",
    "dev": "tsx ./src/cli.ts",
    "test": "npm pack && npm rm -g gobot-age && npm i -g gobot-age-1.1.1.tgz && age --g-refresh --g-vvv --help",
    "build": "npm run clean && tsup-node src/cli.ts src/api.ts --dts src/api.ts --format esm"
  },
  "bin": {
    "age": "./dist/cli.js"
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/benallfree/gobot/tree/main/src/apps/age/helper"
  },
  "keywords": [
    "gobot",
    "age",
    "Age"
  ],
  "author": "Ben Allfree",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benallfree/gobot/issues"
  },
  "homepage": "https://www.npmjs.com/package/gobot-age",
  "dependencies": {
    "gobot": "*"
  },
  "devDependencies": {
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  }
}
