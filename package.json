    "decompress-targz": "^4.1.1",
    "decompress-unzip": "^4.0.1",
    "env-paths": "^3.0.0",
    "fast-safe-stringify": "^2.1.1",
    "file-type": "^19.0.0",
    "glob": "^10.3.10",
    "is-stream": "^4.0.1",
    "json-stringify-safe": "^5.0.1",
    "lzma-native": "^8.0.6",
    "markdown-table": "^3.0.3",
    "node-fetch": "^3.3.2",
    "rimraf": "^5.0.5",
    "semver": "^7.6.0",
    "table": "^6.8.2",
    "tar-stream": "1.6.2",
    "unbzip2-stream": "^1.4.3"
  },
