{
  "name": "gobot",
  "version": "1.0.0-alpha.33",
  "description": "Install and run most any binary app as an npm package dependency.",
  "repository": "github:benallfree/gobot",
  "type": "module",
  "main": "./dist/api.js",
  "bin": {
    "gobot": "./dist/cli.js"
  },
  "types": "dist/api.d.ts",
  "scripts": {
    "clean": "plop clean all && rimraf dist",
    "build:gobot": "tsup-node",
    "plop:transpile": "tsup-node ./plopfile.ts --out-dir .",
    "base": "pnpm clean && concurrently 'pnpm:plop:transpile' 'pnpm:build:gobot'",
    "build": "pnpm base && pnpm build:only",
    "build:only": "plop build all && pnpm lint:fix",
    "paq": "pnpm build && pnpm paq:only",
    "paq:only": "plop pack all",
    "publish": "pnpm paq && pnpm publish:only",
    "publish:only": "plop publish all",
    "plop": "pnpm plop:transpile && plop",
    "watch": "tsup-node --watch .",
    "dev": "pnpm build:gobot && node dist/cli.js",
    "test:pack": "pnpm paq && npm rm -g gobot && npm i -g gobot-`cat package.json | fx '.version'`.tgz && gobot pocketbase --g-refresh --g-vvv --help",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
    "docs": "typedoc",
    "lint": "prettier -c .",
    "lint:fix": "prettier -w ."
  },
  "keywords": [
    "pocketbase",
    "pockethost"
  ],
  "author": {
    "name": "Ben Allfree",
    "url": "https://github.com/benallfree"
  },
  "license": "ISC",
  "dependencies": {
    "@s-libs/micro-dash": "^17.1.0",
    "bottleneck": "^2.19.5",
    "commander": "^12.0.0",
    "decompress": "^4.2.1",
    "decompress-bzip2": "^4.0.0",
    "decompress-gz": "^0.0.1",
    "decompress-tar": "4.1.1",
    "decompress-tarbz2": "^4.1.1",
    "decompress-targz": "^4.1.1",
    "decompress-unzip": "^4.0.1",
    "env-paths": "^3.0.0",
    "file-type": "^19.0.0",
    "glob": "^10.3.10",
    "is-stream": "^4.0.1",
    "json-stringify-safe": "^5.0.1",
    "lzma-native": "^8.0.6",
    "markdown-table": "^3.0.3",
    "node-fetch": "^3.3.2",
    "rimraf": "^5.0.5",
    "semver": "^7.6.0",
    "tar-stream": "1.6.2",
    "unbzip2-stream": "^1.4.3"
  },
  "devDependencies": {
    "@types/decompress": "^4.2.7",
    "@types/jest": "^29.5.12",
    "@types/jest-specific-snapshot": "^0.5.9",
    "@types/json-stringify-safe": "^5.0.3",
    "@types/lzma-native": "^4.0.4",
    "@types/node": "^20.11.28",
    "@types/semver": "^7.5.8",
    "@types/tar-stream": "1.6.2",
    "@types/unbzip2-stream": "^1.4.3",
    "boolean": "^3.2.0",
    "concurrently": "^8.2.2",
    "find-up": "^7.0.0",
    "fx": "^31.0.0",
    "jest": "^29.7.0",
    "jest-specific-snapshot": "^8.0.0",
    "plop": "^4.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "sharp": "^0.33.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "type-fest": "^4.14.0",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.4.2",
    "verdaccio": "^5.29.2",
    "yaml": "^2.4.1"
  },
  "pnpm": {
    "patchedDependencies": {
      "plop@4.0.1": "patches/plop@4.0.1.patch",
      "node-plop@0.32.0": "patches/node-plop@0.32.0.patch"
    }
  }
}
