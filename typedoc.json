{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/api.ts", "./src/Gobot.ts"],
  "out": "./docs",
  "plugin": ["typedoc-plugin-markdown"],
  "hideBreadcrumbs": true,
  "hidePageTitle": true,
  "readme": "none",
  "entryDocument": "readme.md",
  "excludePrivate": true,
  "gitRevision": "main",
  "exclude": ["**/build"],
  "excludeExternals": true,
  "tsconfig": "tsconfig.typedoc.json"
}
